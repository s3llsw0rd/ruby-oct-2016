<h1>Login</h1>
<% if flash[:alert] %>
	<p><%= flash[:alert] %></p>
<% end %>
<%= form_for @session, url: sessions_path, method: :post do |f| %>
	<%= f.label :email %>
	<%= f.email_field :email %>
	<%= f.label :password %>
	<%= f.password_field :password %>
	<%= f.submit :value => "Log In" %>
<% end %>
